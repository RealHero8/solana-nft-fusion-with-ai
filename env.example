# Solana Configuration
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_PRIVATE_KEY=your_private_key
SOLANA_NETWORK=mainnet-beta

# NestJS Backend Configuration
PORT=3000
NODE_ENV=development
DATABASE_URL=postgresql://username:password@localhost:5432/nft_fusion_db

# AI Model Configuration
AI_MODEL_PATH=/path/to/models
GENERATION_QUALITY=high
STABLE_DIFFUSION_MODEL=stabilityai/stable-diffusion-2-1
STYLEGAN_MODEL_PATH=/path/to/stylegan/models

# Storage Configuration
IPFS_GATEWAY=https://ipfs.io/ipfs/
ARWEAVE_URL=https://arweave.net
ARWEAVE_PRIVATE_KEY=your_arweave_private_key

# API Keys
OPENAI_API_KEY=your_openai_key
STABILITY_API_KEY=your_stability_key
REPLICATE_API_KEY=your_replicate_key

# JWT Configuration
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=24h

# Redis Configuration (for caching)
REDIS_URL=redis://localhost:6379

# Logging
LOG_LEVEL=debug 